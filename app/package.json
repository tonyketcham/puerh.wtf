{
	"name": "puerh.wtf",
	"version": "0.0.1",
	"scripts": {
		"preinstall": "npx only-allow pnpm",
		"dev": "flatbread start -- vite dev",
		"prebuild": "test \"$CI\" = true && npx pnpm install -r --store=node_modules/.pnpm-store || echo skiping pnpm install",
		"build": "flatbread start -X npx -- vite build",
		"package": "svelte-kit package",
		"preview": "flatbread start -- vite preview",
		"test": "flatbread start -- playwright test",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint .",
		"format": "prettier --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@giscus/svelte": "2.2.0",
		"@neodrag/svelte": "1.2.3",
		"@playwright/test": "1.24.2",
		"@svelte-plugins/tooltips": "^0.1.6",
		"@sveltejs/adapter-static": "1.0.0-next.39",
		"@sveltejs/kit": "1.0.0-next.405",
		"@tailwindcss/typography": "0.5.4",
		"@typescript-eslint/eslint-plugin": "5.32.0",
		"@typescript-eslint/parser": "5.32.0",
		"autoprefixer": "10.4.8",
		"d3-shape": "3.1.0",
		"eslint": "8.21.0",
		"eslint-config-prettier": "8.5.0",
		"eslint-plugin-svelte3": "4.0.0",
		"flatbread": "1.0.0-alpha.20",
		"graphql-request": "4.3.0",
		"hast-util-parse-selector": "3.1.0",
		"hast-util-select": "5.0.2",
		"plur": "5.1.0",
		"postcss": "^8.4.16",
		"postcss-scrollbar": "0.5.1",
		"prettier": "2.7.1",
		"prettier-plugin-svelte": "2.7.0",
		"svelte": "3.49.0",
		"svelte-check": "2.8.0",
		"svelte-portal": "2.2.0",
		"svelte-preprocess": "4.10.7",
		"tailwindcss": "3.1.8",
		"tailwindcss-textshadow": "2.1.3",
		"tslib": "2.4.0",
		"typescript": "4.7.4",
		"unist-util-visit": "4.1.0",
		"vite": "3.0.4"
	},
	"type": "module",
	"dependencies": {
		"@types/d3-shape": "^3.1.0",
		"layercake": "^7.1.0"
	}
}
